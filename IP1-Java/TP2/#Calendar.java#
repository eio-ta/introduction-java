    public static int JulianToCounter(int year, int month, int day) {

	int a = 0;
	for (int i=1; i<year; i = i + 1) {
	    int n = year 
	    int modelo = year % 4;
	    if (modelo == 0) {
		modelo = 366;
	    }
	    else {
		modelo = 365;
	    }
	    int n = modelo 
	}
	    if (
		a = a + n; }

	int b = 0;
	for (int j=1; j<month; j = j + 1) {
	    b = month - 0;}

	int numdays = day - 2;
	return (a + b + numdays); }
	
    


