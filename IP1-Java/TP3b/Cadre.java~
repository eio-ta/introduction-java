public class Cadre {
    
    /* Écrivez vos fonctions ici */
    public static void line (n); {
	int nn = 0;
	for (int i = 0; i < n; i = i + 1) {
	    System.out.print("#");
	}
	System.out.println();
    }
    
    public static void main(String[] args) {
	/* Écrivez vos tests ici */
	line (7);
	
    }
}
