public class Eratosthene {

    // Ã‰crivez vos fonctions ici

    public static int premier (int p) {

        for(int i=2; i<p; i++) {
            if (p%i == 0) {
                return 0;
            }
        }
        return p;
    }


    public static int[] crible (int n) {

        int[] era = new int [n-1];
        int number = 2;

        for(int i=0; i<era.length; i++) {
            era[i] = premier(number);
            number = number+1;
        }
        return era;
    }

    /*public static int[] prime (int n) {

        int[] prime = new int [n];
        int number = 2;
        

            for(int i=0; i<prime.length; i++) {
                prime[i] = premier(number);
                number = number+1;
            }
        }

        return prime;
    }*/


    public static void printIntArray (int[] a){

	    for(int i = 0; i<a.length; i++){
	        System.out.print(a[i] + " ");
	    }
	    System.out.println();
    }

    public static void main(String[] args) {
        printIntArray(crible(10));
        //printIntArray(prime(10));
        
    }
}
